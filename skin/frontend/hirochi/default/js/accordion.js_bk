$(function(){
$(".accordion ul").hide();
$(".accordion p").click(function(){
$(this).next("ul").slideToggle();
    $(this).children("span").toggleClass("open");
});
$(".nav ul").hide();
$(".nav p").click(function(){
$(this).next("ul").slideToggle();
    $(this).children("span").toggleClass("open");
});
 
});